                                        <td><span id="t-pen-depassement-badge" class="badge badge-danger"><span id="t-pen-depassement-status">Oui</span></span></td> #}
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td>Mauvais FP</td>
                                        <td>0.8</td>
                                        <td id="t-pen-fp">{{dataAnalysis['recapProd']['gridData']['FP']|number_format(2, '.', ' ')}}</td>
