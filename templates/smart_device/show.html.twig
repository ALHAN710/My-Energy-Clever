{% extends 'base.html.twig' %}

{% block title %}SmartDevice{% endblock %}

{% block body %}
    <h1>SmartDevice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ smart_device.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ smart_device.name }}</td>
            </tr>
            <tr>
                <th>Specification</th>
                <td>{{ smart_device.specification }}</td>
            </tr>
            <tr>
                <th>ModuleId</th>
                <td>{{ smart_device.moduleId }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ smart_device.slug }}</td>
            </tr>
            <tr>
                <th>Programming</th>
                <td>{{ smart_device.programming ? smart_device.programming|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ smart_device.createdAt ? smart_device.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EditedAt</th>
                <td>{{ smart_device.editedAt ? smart_device.editedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ smart_device.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('smart_device_index') }}">back to list</a>

    <a href="{{ path('smart_device_edit', {'id': smart_device.id}) }}">edit</a>

    {{ include('smart_device/_delete_form.html.twig') }}
{% endblock %}
