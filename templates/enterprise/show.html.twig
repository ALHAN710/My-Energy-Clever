{% extends 'base.html.twig' %}

{% block title %}Enterprise{% endblock %}

{% block body %}
    <h1>Enterprise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enterprise.id }}</td>
            </tr>
            <tr>
                <th>SocialReason</th>
                <td>{{ enterprise.socialReason }}</td>
            </tr>
            <tr>
                <th>Niu</th>
                <td>{{ enterprise.niu }}</td>
            </tr>
            <tr>
                <th>Rccm</th>
                <td>{{ enterprise.rccm }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ enterprise.address }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ enterprise.email }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ enterprise.phoneNumber }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ enterprise.createdAt ? enterprise.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ enterprise.logo }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ enterprise.country }}</td>
            </tr>
            <tr>
                <th>EditedAt</th>
                <td>{{ enterprise.editedAt ? enterprise.editedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ enterprise.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ enterprise.slug }}</td>
            </tr>
            <tr>
                <th>AccountType</th>
                <td>{{ enterprise.accountType }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('enterprise_index') }}">back to list</a>

    <a href="{{ path('enterprise_edit', {'id': enterprise.id}) }}">edit</a>

    {{ include('enterprise/_delete_form.html.twig') }}
{% endblock %}
